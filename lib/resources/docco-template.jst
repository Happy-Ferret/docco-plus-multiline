<!DOCTYPE html>

<html>
<head>
    <!-- FIXME: title -->
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport"
        content="width=device-width, target-densitydpi=160dpi, initial-scale=1.0; maximum-scale=1.0; user-scalable=0;">
    <link rel="stylesheet" href="jstree/themes/default-dark/style.min.css">
    <link rel="stylesheet" href="docco-template.css">
    <% if (options.css !== 'null') { %>
    <link rel="stylesheet" media="all" href="custom.css"/>
    <% } %>
    <script src="jquery.min.js" type="text/javascript"></script>
    <script src="jstree/jstree.min.js" type="text/javascript"></script>
    <script src="tree-builder.js" type="text/javascript"></script>
</head>
<body>
<h1><%=srcPath %></h1>
<nav>
</nav>
<section>
    <ul>
    <% for (var i=0, l=sections.length; i<l; i++) { %>
    <% var section = sections[i]; %>
        <li id="section-<%= i + 1 %>">
            <div class="annotation">
                <%= section.docsHtml %>
            </div>
            <div class="content"><%= section.codeHtml %></div>
        </li>
    <% } %>
    </ul>
</section>
</body>
</html>
